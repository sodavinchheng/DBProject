<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">
    <servlet>
        <servlet-name>InsertServlet</servlet-name>
        <servlet-class>InsertRecordServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>InsertServlet</servlet-name>
        <url-pattern>/InsertRecord</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ViewServlet</servlet-name>
        <servlet-class>ViewRecordServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ViewServlet</servlet-name>
        <url-pattern>/ViewRecord</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>UpdateServlet</servlet-name>
        <servlet-class>UpdateRecordServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>UpdateServlet</servlet-name>
        <url-pattern>/UpdateRecord</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DeleteServlet</servlet-name>
        <servlet-class>DeleteRecordServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>DeleteServlet</servlet-name>
        <url-pattern>/DeleteRecord</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ViewAllServlet</servlet-name>
        <servlet-class>ViewAllRecordsServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ViewAllServlet</servlet-name>
        <url-pattern>/ViewAllRecords</url-pattern>
    </servlet-mapping>
</web-app>
